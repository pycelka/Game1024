<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".StartActivity">


    <Button

        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button"
        android:layout_marginStart="49dp"
        android:layout_marginTop="160dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button"
        android:layout_toEndOf="@+id/button" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_toEndOf="@+id/button2" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button4"
        android:layout_alignTop="@+id/button3"
        android:layout_toEndOf="@+id/button3" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button5"
        android:layout_below="@+id/button"
        android:layout_alignStart="@+id/button" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button6"
        android:layout_below="@+id/button"
        android:layout_toEndOf="@+id/button5" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button7"
        android:layout_below="@+id/button2"
        android:layout_toEndOf="@+id/button6" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button8"
        android:layout_below="@+id/button3"
        android:layout_toEndOf="@+id/button7" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button9"
        android:layout_below="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button10"
        android:layout_alignTop="@+id/button9"
        android:layout_toEndOf="@+id/button9" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button11"
        android:layout_alignTop="@+id/button10"
        android:layout_toEndOf="@+id/button10" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button12"
        android:layout_alignTop="@+id/button11"
        android:layout_alignStart="@+id/button8" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button13"
        android:layout_below="@+id/button9"
        android:layout_alignStart="@+id/button9" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button14"
        android:layout_alignTop="@+id/button13"
        android:layout_toEndOf="@+id/button13" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button15"
        android:layout_below="@+id/button11"
        android:layout_toStartOf="@+id/button12" />

    <Button
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/button16"
        android:layout_alignTop="@+id/button15"
        android:layout_alignStart="@+id/button12" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Losuj"
        android:id="@+id/losuj_button"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button17"
        android:layout_above="@+id/button2"
        android:layout_toEndOf="@+id/button"
        android:layout_marginBottom="32dp" />

</RelativeLayout>
